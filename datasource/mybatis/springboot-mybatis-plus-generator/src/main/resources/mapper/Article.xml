<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.zingfin.finance.mapper.ArticleMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.zingfin.finance.repository.entity.Article">
    <result column="id" property="id" />
    <result column="type" property="type" />
    <result column="status" property="status" />
    <result column="is_valid" property="isValid" />
    <result column="op_time" property="opTime" />
    <result column="last_ver" property="lastVer" />
    <result column="create_time" property="createTime" />
    <result column="app_id" property="appId" />
        <result column="ext" property="ext" />
        <result column="title" property="title" />
        <result column="sub_title" property="subTitle" />
        <result column="content" property="content" />
        <result column="create_id" property="createId" />
        <result column="cate_id" property="cateId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        type,
        status,
        is_valid,
        op_time,
        last_ver,
        create_time,
        app_id,
        ext, title, sub_title, content, create_id, cate_id
    </sql>

</mapper>
